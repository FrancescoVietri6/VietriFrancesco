<?xml version="1.0" encoding="ISO-8859-1"?>


<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="my.universityuiip.data.StudentData">
		<property name="serialNumber" type="String"/>
		<property name="fullName" type="String"/>
		<property name="birthDate" type="java.util.Date"/>
	</bean>
	
	<bean class="my.universityuiip.data.ProfessorData">
		<property name="serialNumber" type="String"/>
		<property name="fullName" type="String"/>
		<property name="birthDate" type="java.util.Date"/>
	</bean>
	
	<bean class="my.universityuiip.data.ExamData">
		<property name="uid" type="String"/>
		<property name="result" type="Enum"/>
		<property name="grade" type="Integer"/>
		<property name="subject" type="my.universityuiip.data.SubjectData"/>
	</bean>
	
	<bean class="my.universityuiip.data.SubjectData">
		<property name="uid" type="String"/>
		<property name="name" type="String"/>
		<property name="program" type="String"/>
	</bean>
	
</beans>
